<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weimin.association.dao.StudentDao">
    <select id="getByTid" resultType="com.weimin.association.pojo.Student">
        select *
        from student
        where tid = #{tid}
    </select>
</mapper>